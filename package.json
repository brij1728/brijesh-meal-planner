{
  "name": "Meal-Crafters",
  "version": "1.0.0",
  "main": "index.tsx",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android:preview": "eas build --local -p android --profile preview --output=./app.brijesh.mealcrafters-preview-build.apk --non-interactive",
    "build:android:development": "eas build --local -p android --profile development --output=./app.brijesh.mealcrafters-development-build.apk --non-interactive",
    "build:ios:preview": "eas build --local -p ios --profile preview --output=./app.brijesh.mealcrafters-preview-build.ipa --non-interactive",
    "build:ios:development": "eas build --local -p ios --profile development --output=./app.brijesh.mealcrafters-development-build.ipa --non-interactive",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint ./ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint ./ --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/stack": "^6.3.17",
    "expo": "~48.0.18",
    "expo-dev-client": "~2.2.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/react": "~18.0.14",
    "eslint": "^8.42.0",
    "eslint-config-universe": "^11.2.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "typescript": "^4.9.4"
  },
  "private": true
}
